{
    "name": "septnn/septnndoc",
    "type": "project",
    "license": "MIT",
    "description": "Documentation generator that uses a simple folder structure and Markdown files to create custom documentation on the fly",
    "keywords": ["documentation", "docs", "markdown", "md"],
    "authors": [
        {
            "name": "septnn",
            "homepage": "https://github.com/sepntt"
        }
    ],
    "bin": ["bin/daux"],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "~6.0",
        "league/commonmark": "^0.18",
        "league/plates": "~3.1",
        "myclabs/deep-copy": "^1.5",
        "symfony/console": "^4.0",
        "symfony/http-foundation": "^4.0",
        "symfony/polyfill-intl-icu": "^1.10",
        "symfony/process": "^4.0",
        "webuni/commonmark-table-extension": "0.9.*",
        "webuni/front-matter": "^1.0.0",
        "swiftmailer/swiftmailer" : "^6.2.1",
        "phpseclib/phpseclib": "^2.0"

    },
    "suggest":{
        "ext-intl": "Allows to translate the modified at date"
    },
    "autoload": {
        "psr-4": {
            "Todaymade\\Daux\\": "libs/"
        }
    },
    "replace": {
        "justinwalsh/daux.io": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4",
        "mikey179/vfsstream": "^1.6"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
